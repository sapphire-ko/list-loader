{
  "name": "list-loader",
  "version": "1.0.8",
  "description": "text list loader module for webpack",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build",
    "prepublish": "npm run build",
    "build": "cross-env NODE_ENV=prod babel ./src -d ./dist",
    "eslint": "eslint .",
    "test": "jest --no-cache --detectOpenHandles",
    "coverage": "jest --coverage --no-cache --detectOpenHandles",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapphiredev/list-loader.git"
  },
  "keywords": [
    "webpack-loader"
  ],
  "author": "sapphire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapphiredev/list-loader/issues"
  },
  "homepage": "https://github.com/sapphiredev/list-loader#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "jest": "^23.5.0",
    "webpack": "^4.17.0"
  },
  "dependencies": {
    "loader-utils": "^1.1.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test"
    ],
    "testEnvironment": "node"
  }
}
